<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>


<Pane fx:id="pane" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.samp.guiv2.Scene4Controller">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="600.0">
         <children>
            <DatePicker fx:id="datePicker" layoutX="14.0" layoutY="90.0" onAction="#getDate" promptText="Enter a Date" />
            <Label fx:id="label" alignment="CENTER" layoutX="14.0" layoutY="14.0" prefHeight="76.0" prefWidth="174.0" text="Date of Birth">
               <font>
                  <Font name="Bodoni MT Black" size="18.0" />
               </font>
            </Label>
            <ColorPicker fx:id="colorPicker" layoutX="238.0" layoutY="355.0" onAction="#colorPick">
               <value>
                  <Color red="0.009999999776482582" green="0.009999999776482582" blue="0.009999999776482582" />
               </value>
            </ColorPicker>
            <ChoiceBox fx:id="choiceBox1" layoutX="393.0" layoutY="52.0" prefWidth="150.0" />
            <ChoiceBox fx:id="choiceBox2" layoutX="393.0" layoutY="103.0" prefWidth="150.0" />
            <Label fx:id="choiceBoxLabel" alignment="CENTER" layoutX="263.0" layoutY="44.0" prefHeight="42.0" prefWidth="100.0" text="choiceBox1">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="choiceBoxLabel2" alignment="CENTER" layoutX="263.0" layoutY="95.0" prefHeight="42.0" prefWidth="100.0" text="choiceBox2">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</Pane>
