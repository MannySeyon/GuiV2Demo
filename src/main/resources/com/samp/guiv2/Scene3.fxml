<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pizzButton" prefHeight="400.0" prefWidth="600.0" stylesheets="@Scene3.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.samp.guiv2.Scene3Controller">
   <children>
      <Button fx:id="switchToScene2" layoutX="21.0" layoutY="327.0" mnemonicParsing="false" onAction="#switchToScene2" prefHeight="53.0" prefWidth="126.0" text="SwItch to Scene 2" />
      <Button fx:id="switchToMain" layoutX="476.0" layoutY="14.0" mnemonicParsing="false" onAction="#switchToMain" prefHeight="39.0" prefWidth="110.0" text="To Main Screen" />
      <RadioButton fx:id="rButton0" layoutX="84.0" layoutY="181.0" mnemonicParsing="false" onAction="#getFood" prefHeight="39.0" prefWidth="80.0" text="Pizza">
         <toggleGroup>
            <ToggleGroup fx:id="Food" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="rButton1" layoutX="261.0" layoutY="181.0" mnemonicParsing="false" onAction="#getFood" prefHeight="39.0" prefWidth="80.0" text="Ramen" toggleGroup="$Food" />
      <RadioButton fx:id="rButton2" layoutX="436.0" layoutY="181.0" mnemonicParsing="false" onAction="#getFood" prefHeight="39.0" prefWidth="80.0" text="Sushi" toggleGroup="$Food" />
      <Label fx:id="meRadioLabel" alignment="CENTER" layoutX="22.0" layoutY="90.0" prefHeight="73.0" prefWidth="557.0" text="Label" textFill="#e8dada">
         <font>
            <Font name="Bodoni MT" size="24.0" />
         </font></Label>
   </children>
</AnchorPane>
